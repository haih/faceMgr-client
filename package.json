{
  "name": "ssm-client",
  "version": "1.0.0",
  "description": "ssm client UI",
  "author": "haih",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && webpack --bail --progress --profile",
    "clean-cache": "npm cache clean",
    "clean": "rm -rf node_modules dist ./test/results ./test/coverage",
    "clean-install": "npm run clean && npm install",
    "kill": "lsof -i tcp:8000 | grep LISTEN | awk '{print $2}' | xargs kill",
    "lint": "eslint src",
    "lint-quiet": "eslint --quiet src",
    "open-dev": "opn http://127.0.0.1:8000 -- 'google chrome'",
    "package-dist": "./bin/package-dist.sh",
    "server-dist": "lite-server -c config/lite-server.json",
    "server": "webpack-dev-server --history-api-fallback --inline --progress",
    "start-dist": "npm run build & npm run open-dev & npm run server-dist",
    "start": "npm run open-dev & npm run server",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/haih/ssm-client"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.5",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "copy-webpack-plugin": "^2.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^0.5.5",
    "karma-htmlfile-reporter": "^0.2.2",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lite-server": "^2.2.0",
    "ng-annotate-loader": "^0.1.0",
    "ng-cache-loader": "0.0.15",
    "node-sass": "^3.5.3",
    "opener": "^1.4.1",
    "opn-cli": "^3.1.0",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.8.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "angular": "~1.5.8",
    "angular-animate": "~1.5.8",
    "angular-cookies": "^1.5.5",
    "angular-messages": "~1.5.5",
    "angular-recaptcha": "~3.0.3",
    "angular-resource": "^1.5.5",
    "angular-sanitize": "~1.5.5",
    "angular-translate": "^2.11.0",
    "angular-translate-interpolation-messageformat": "^2.11.0",
    "angular-translate-loader-static-files": "^2.11.0",
    "angular-ui-router": "1.0.0-alpha.4",
    "angular-utf8-base64": "0.0.2",
    "lodash": "~4.11.1",
    "jquery": "2.1.1",
    "ng-focus-if": "~1.0.7",
    "uuid": "^2.0.2",
    "oclazyload": "~1.0.9"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.0.0 <3.0"
  }
}
